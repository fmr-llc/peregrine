<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

		
	<bean id="publishDateTime1" class="org.joda.time.DateTime">
	    	<constructor-arg type="java.lang.Object" value="2015-09-08T13:40:47.285-0400" />
	</bean>
	<bean id="receivedDateTime1" class="org.joda.time.DateTime">
	    	<constructor-arg type="java.lang.Object" value="2015-09-15T13:40:47.285-0400" />
	</bean>
	<bean id="expirationDateTime1" class="org.joda.time.DateTime">
	    	<constructor-arg type="java.lang.Object" value="2015-09-15T13:40:47.285-0400" />
	</bean>
	<bean id="insertDateTime1" class="org.joda.time.DateTime">
	    	<constructor-arg type="java.lang.Object" value="2015-09-08T13:40:47.285-0400" />
	</bean>
	<bean id="publishDateTime2" class="org.joda.time.DateTime">
	    	<constructor-arg type="java.lang.Object" value="2015-09-08T13:40:47.285-0400" />
	</bean>
	<bean id="receivedDateTime2" class="org.joda.time.DateTime">
	    	<constructor-arg type="java.lang.Object" value="2015-09-09T10:07:18.285-0400" />
	</bean>
	<bean id="expirationDateTime2" class="org.joda.time.DateTime">
	    	<constructor-arg type="java.lang.Object" value="2015-09-16T10:07:18.285-0400" />
	</bean>
	<bean id="insertDateTime2" class="org.joda.time.DateTime">
	    	<constructor-arg type="java.lang.Object" value="2015-09-09T10:07:18.285-0400" />
	</bean>
	
	<bean id="parentEvent" class="com.alliancefoundry.model.Event">
		<property name="parentId"><null/></property>
		<property name="eventName" value="Testing name" />
		<property name="objectId" value="some object id" />
		<property name="correlationId" value="some correlation id" />
		<property name="sequenceNumber" value="1" /> <!-- Integer -->
		<property name="messageType" value="kafka - testJaneDoe344" />
		<property name="dataType" value="Object" />
		<property name="source" value="some source" />
		<property name="destination" value="Kafka" />
		<property name="subdestination" value="test2" />
		<property name="replayIndicator" value="false" /> <!-- Boolean -->
		<property name="publishTimeStamp" ref="publishDateTime1" /> <!-- DateTime -->
		<property name="receivedTimeStamp" ref="receivedDateTime1" /> <!-- DateTime --> 
		<property name="expirationTimeStamp" ref="expirationDateTime1" /> <!-- DateTime -->
		<property name="preEventState" value="pre" />
		<property name="postEventState" value="post" />
		<property name="isPublishable" value="true" /> <!-- Boolean -->
		<property name="insertTimeStamp" ref="insertDateTime1" /> <!-- DateTime -->
	</bean>
	
